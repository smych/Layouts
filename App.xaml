<Application x:Class="LayotsMvvm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LayotsMvvm"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style\Colors.xaml" />
                <ResourceDictionary Source="Style\Buttons.xaml" />
                <ResourceDictionary Source="Style\Fonts.xaml" />
                <ResourceDictionary Source="Style\DefaultBlock.xaml" />
                <ResourceDictionary Source="Style\ListView.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
